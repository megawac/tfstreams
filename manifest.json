{
   "version": "1.1.2",
   "manifest_version": 2,

   "description": "Displays streams from teamfortress.tv. Grabs streams from http://teamfortress.tv/rss/streams",
   "name": "TF TV Streams",
   "update_url": "http://clients2.google.com/service/update2/crx",
   "options_page": "options.html",

   "permissions": [ "http://teamfortress.tv/*/" ],
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   },
   "background": {
      "scripts": ["js/background.js"]
   },
   "icons":{
      "128":"ext.png"
   }
}
